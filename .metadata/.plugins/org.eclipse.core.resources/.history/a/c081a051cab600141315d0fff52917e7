package com.sizemore.codility;

import java.util.Arrays;

public class Challenge {
	
	public Challenge(int K, int[] C, int[] D) {
		int[] to = C;
		int[] from = new int[C.length];
		int[] sorted = D;
		Arrays.sort(sorted);
	}

	public static void main (String[] args) {
		int K = 2;
		int[] C = {1,3,0,3,2,4,4};
		int[] D = {6,2,7,5,6,5,2};
		new Challenge(K,C,D);
	}
}
